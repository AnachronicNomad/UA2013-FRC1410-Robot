#include "Shooter.h"
#include "../Robotmap.h"

Shooter::Shooter() : Subsystem("Shooter") {
	shooterMotor = new Jaguar(3);
	altitude = new Jaguar(4);
	hopperRot = new Jaguar(5);
	feeder = new DoubleSolenoid(1,2);
	
	feeder->Set(DoubleSolenoid::kReverse);
	firing = false;
}
    
